# Android fstab file.
#<src>                                                 <mnt_point>            <type>    <mnt_flags and options>                   <fs_mgr_flags>
system                                                  /system                ext4      ro,barrier=1,discard                      wait,slotselect,avb=vbmeta,logical,first_stage_mount
vendor                                                  /vendor                ext4      ro,barrier=1,discard                      wait,slotselect,avb=vbmeta,logical,first_stage_mount
product                                                 /product               ext4      ro,barrier=1,discard                      wait,slotselect,avb=vbmeta,logical,first_stage_mount
/dev/block/by-name/boot                                 /boot                  emmc      defaults                                  recoveryonly
/dev/block/by-name/recovery                             /recovery              emmc      defaults                                  recoveryonly
/dev/block/by-name/userdata                             /data                  f2fs      nosuid,nodev,noatime,discard,reserve_root=32768,resgid=1065 wait,check,forceencrypt=/dev/block/by-name/metadata,quota,reservedsize=128M
